{
  "name": "ssid-to-slack-status",
  "product": {
    "name": "SSID to Slack status",
    "url": ""
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "upgrade": "npm install && npm prune && echo 'Dependencies upgraded' && crontab -l 2> /dev/null | grep -q '# ssid-to-slack-status' && npm run crontab-reinstall || exit 0",
    "update": "node --harmony src/utils/update.js update",
    "force-update": "FORCE_UPDATE=true node --harmony src/utils/update.js force-update",
    "crontab": "node --harmony src/utils/crontab.js",
    "crontab-install": "node --harmony src/utils/crontab.js install",
    "crontab-uninstall": "node --harmony src/utils/crontab.js uninstall",
    "crontab-reinstall": "node --harmony src/utils/crontab.js reinstall",
    "start": "./node_modules/.bin/electron .",
    "watch": "./node_modules/.bin/webpack --watch",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "build-and-run-electron": "./node_modules/.bin/webpack && npm run start"
  },
  "repository": {
    "url": "https://github.com/kirbo/ssid-to-slack-status.git",
    "type": "git"
  },
  "author": "kimmo.saari@sc5.io",
  "license": "MIT",
  "devDependencies": {
    "antd": "2.9.2",
    "babel": "6.23.0",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-import": "1.1.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.17.1",
    "css-loader": "0.28.0",
    "dotenv": "4.0.0",
    "electron": "1.6.5",
    "electron-packager": "8.6.0",
    "electron-react-devtools": "0.4.0",
    "electron-rebuild": "1.5.7",
    "express": "4.15.2",
    "express-session": "1.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "node-emoji": "1.5.1",
    "node-libs-browser": "2.0.0",
    "node-sass": "4.5.2",
    "node-wifi": "git+https://github.com/kirbo/node-wifi.git",
    "postcss-loader": "1.3.3",
    "raw-loader": "0.5.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-intl": "2.2.3",
    "sass": "0.5.0",
    "sass-loader": "6.0.3",
    "slack": "8.2.7",
    "socket.io": "1.7.3",
    "socket.io-client": "1.7.3",
    "source-map-loader": "0.2.1",
    "style-loader": "0.16.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.4"
  }
}
