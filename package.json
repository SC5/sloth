{
  "name": "ssid-to-slack-status",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "upgrade": "npm install && npm prune && echo 'Dependencies upgraded' && crontab -l 2> /dev/null | grep -q '# ssid-to-slack-status' && npm run crontab-reinstall || exit 0",
    "update": "$(which node) --harmony src/utils/update.js update",
    "force-update": "FORCE_UPDATE=true $(which node) --harmony src/utils/update.js force-update",
    "crontab": "$(which node) --harmony src/utils/crontab.js",
    "crontab-install": "$(which node) --harmony src/utils/crontab.js install",
    "crontab-uninstall": "$(which node) --harmony src/utils/crontab.js uninstall",
    "crontab-reinstall": "$(which node) --harmony src/utils/crontab.js reinstall",
    "start": "./node_modules/.bin/electron .",
    "watch": "./node_modules/.bin/webpack-dev-server",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "url": "https://github.com/kirbo/ssid-to-slack-status.git",
    "type": "git"
  },
  "author": "kimmo.saari@sc5.io",
  "license": "MIT",
  "dependencies": {
    "antd": "2.9.2",
    "electron": "1.6.5",
    "node-emoji": "1.5.1",
    "node-wifi": "git+https://github.com/kirbo/node-wifi.git",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-intl": "2.2.3",
    "slack": "8.2.7"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.28.0",
    "electron-packager": "8.6.0",
    "electron-rebuild": "1.5.7",
    "node-libs-browser": "2.0.0",
    "node-sass": "4.5.2",
    "sass": "0.5.0",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.4"
  }
}
